
  <style>
    #line_chart {  
    height: 1px;
     margin-top: opx;
     margin-left: 25px; }
    * {
      box-sizing: border-box;
    }
    
    input[type=text], select, file {
      width: 70%;
      padding: 16px;
      border: 1px solid #ccc;
      border-radius: 4px;
      resize: vertical;
      margin-left:150px;
    }
    input[type=date]{
        width: 35%;
      padding: 12px;
      border: 1px solid #ccc;
      border-radius: 4px;
      resize: vertical;
      margin-left:150px;
    }
    input[type=file]{
        width: 30%;
      padding: 12px;
      border: 1px solid rgb(111, 173, 207);
      resize: vertical;
      margin-left:150px;
    }
    
    label {
      padding: 12px 12px 12px 0;
      display: inline-block;
      margin-left:160px;
    }
    
    input[type=submit] {
      background-color: #04AA6D;
      color: white;
      padding: 12px 20px;
      border: none;
      border-radius: 4px;
      cursor: pointer;
      float: right;
      margin-right: 100px;
    }
    
    input[type=submit]:hover {
      background-color: #45a049;
    }
    
    .container {
      border-radius: 5px;
      background-color: #f2f2f2;
      padding: 30px;
    }
    
    .col-25 {
      float: left;
      width: 25%;
      margin-top: 6px;
    }
    
    .col-75 {
      float: left;
      width: 75%;
      margin-top: 6px;
    }
    
    /* Clear floats after the columns */
    .row:after {
      content: "";
      display: table;
      clear: both;
    }
    #ss{
      margin-top: 20px;
    }
    
    /* Responsive layout - when the screen is less than 600px wide, make the two columns stack on top of each other instead of next to each other */
    @media screen and (max-width: 600px) {
      .col-25, .col-75, input[type=submit] {
        width: 100%;
        margin-top: 0;
      }
    }
    </style>

<section class="content dashboard">
    <div class="main-graph">
            <div class="row clearfix">
                <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                    <div class="graph-area">
                         <div id="line_chart"></div>
                    </div> 
                </div>
              
            </div>
        </div>
        <!-- #END# Main Graph-->
    <div class="page-heading">
        <h1>Contact Us Edit </h1>
        <ol class="breadcrumb">
            <li><a href="/public/index">Home</a></li>
            <li class="active">Contact Us</li>
        </ol>
    </div>


    <div class="container">
      <form action="" method="POST">
        <input type="hidden" name="id" value="<%= data.id %>">
        <div class="row">
          <div class="col-25">
            <label for="fname">Client name</label>
          </div>
          <div class="col-75">
            <input type="text" id="clint_name" name="client_name" value="<%= data.client_name %>" required>
          </div>
        </div>
        <div class="row">
          <div class="col-25">
            <label for="lname">Phone</label>
          </div>
          
          <div class="col-75">
            <input type="text" id="mobile" name="phone" value="<%= data.phone %>" required>
          </div>
        </div>

          <div class="row">
            <div class="col-25">
              <label for="lname">Phone</label>
            </div>
          <div class="col-75">
            <input type="text" name="whatsapp" value="<%= data.whatsapp %>" required>
          </div>

        </div>

        <div class="row">
          <div class="col-25">
            <label for="country">Email</label>
          </div>
          <div class="col-75">
            <input type="text" id="email" name="email"value="<%= data.email%>" required>
             
          </div>
        </div>
       
        <div class="row">
          <input type="submit" value="Submit" id="ss">
        </div>
      </form>
    </div>
    
   
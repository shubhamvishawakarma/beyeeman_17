      <section class="content dashboard">
            <div class="main-graph">
                    <div class="row clearfix">
                        <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                            <div class="graph-area">
                                 <div id="line_chart"></div>
                            </div> 
                        </div>
                      
                    </div>
                </div>
                <!-- #END# Main Graph-->
            <div class="page-heading">
                <h1>Push Notification List</h1>
                <ol class="breadcrumb">
                    <li><a href="/public/index">Home</a></li>
                    <li class="active">Push Notification List</li>
                </ol>
            </div> 


            <div class="panel panel-default">
            
                <div class="panel-heading"></div>
                     <button class="btn btn-info add-new" id="bbt"><i class="fa fa-plus"></i> <a href="/public/send_notification">Send Notification</a></button> 
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table id="myTable" class="table js-exportable table-bordered">
                            
                            <thead>
                                <tr> 
                                    <th>S.No</th>
                                    <th>Title</th> 
                                    <th>Type</th>   
                                    <th>Description</th>
                                    <th>Image</th>
                                    <!--<th>Edit</th>-->
                                    <th>Delete</th>
                                </tr> 
                            </thead>
                            <tbody> 
                                <% for(var i=0; i < data.length; i++) { %>

                                <tr>
                                    <td><%= i+1%></td>
                                    <td><%=data[i].title%></td>
                                    <td><%=data[i].type%></td>
                                    <td><%=data[i].notification%></td>
                                    <td><img src="/uploads/<%=data[i].image%>" alt="img" id="imgs"></td>
                                    <!--<td><a href="/public/edit_push_notification/<%=data[i]._id%>" class="btn btn-primary">Edit</a>
                                    </td>-->
                                    <td><a href="/public/delete_push_notification/<%=data[i]._id%>" class="btn btn-danger">Delete</a>
                                    </td>
                                </tr>
                            <% } %>
                            </tbody> 
                        
                            </table>
                        </div>
                    </div>
                </div>
                <!-- #END# Task data -->
            </div>
        </section>
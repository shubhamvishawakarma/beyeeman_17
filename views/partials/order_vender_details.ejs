            <div class="page-heading">
                <h1>Order List ( <%=list.shop_name%> )</h1>
                <ol class="breadcrumb">
                    <li><a href="/public/index">Home</a></li>
                    <li class="active">Order List</li>
                </ol>
            </div>


            <div class="panel panel-default">
            
  <div class="panel-heading"></div>
           
                    <div class="panel-body">
                        <ul class="nav nav-tabs" role="tablist">
                            <li>
                             <a href="/public/shop_vender_menu/<%=list._id%>" role="tab" >Shop Details</a></li>
                            <li><a href="/public/push_notification_service/<%=list._id%>" role="tab" >Push notification</a></li>
                            <li><a href="/public/delivery_boy_vender_list/<%=list._id%>" role="tab" >Delivery boy</a></li>
                            <li><a href="/public/user_qr_details/<%=list._id%>" role="tab" >Users</a></li>
                            <li><a href="/public/product_vender_list/<%=list._id%>"" role="tab" >Products</a></li>
                            <li><a href="/public/shop_vender_banner_list/<%=list._id%>" role="tab" >Banner</a></li>
                            <li><a href="/public/report_vender_details/<%=list._id%>" role="tab" >Report</a></li>
                           
                            <li class="active"><a href="/public/order_vender_details/<%=list._id%>" role="tab" >Order</a></li>
                            <li><a href="/public/vender_setting_details/<%=list._id%>" role="tab" >Setting</a></li>
                        </ul>
                        <br>
                        <div class="table-responsive">
                            <table id="myTable" class="table js-exportable table-bordered">
                            
                            <thead>
                                    <tr>
                                        <th>S.No</th>
                                        <th>Order No</th>
                                        <th>User Name</th>
                                        <th>Mobile No</th>
                                        <th>Address</th>
                                        <th>Shop Name</th>
                                        <th>No of Product</th>    
                                        <th>Total Ammount</th>
                                        <th>Order Date</th>
                                        <th>Order Status</th>
                                        <th>View Details</th>
                                       <!--<th>Action</th>
                                        <th>Delete</th>-->
                                    </tr>
                                </thead>
                                <tbody>
                                 <% for(var i=0; i < data.length; i++) { %>
                                 <tr>
                                 <td><%= i+1%></td>
                                 <td><%=data[i].order_no%></td>
                                 <td><%=data[i].userId.user_name%></td>
                                 <td><%=data[i].userId.phone%></td>
                                
                                 <td><%=data[i].addressId.address%></td>
                                  <td><%=data[i].shopId.shop_name%></td>
                                  <td><%= data[i].cartId ? data[i].cartId.products.length : '0' %></td>
                                 <td><%=data[i].grand_total%></td>
                                 <td><%=data[i].order_date%></td>
                                 <td><%=data[i].order_status%></td>
                                 <td><a href="/public/view_vender_order_details/<%=data[i]._id%>" class="btn btn-primary">View</a></td>
                                 <!--<td><a href="/public/edit_order/<%=data[i]._id%>" class="btn btn-primary">Edit</a></td>
                                 <td><a href="/public/delete_order/<%=data[i]._id%>" class="btn btn-danger">Delete</a></td>-->
                                 </tr>

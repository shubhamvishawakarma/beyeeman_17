                    <div class="panel-body">
                        <div class="table-responsive">
                            <table id="myTable" class="table js-exportable table-bordered">
                            
                            <thead> 
                    <tr>
                    <th>S.No</th>
                    <th>Image</th>
                    <th>Brand</th>  
                    <th>Product name</th> 
                    <th>Barcode</th>
                    <th>Weight</th>
                    <th>Description</th>
                    <th>Status</th>              
                    <th>Update</th>               
                    <!-- <th>Delete</th> -->
        </tr>
    </thead>
        <tbody>
        <% for(var i=0; i < data.length; i++) { %>
                                   
        <tr>
         <td><%= i+1%></td>
         <td><img src="/uploads/<%=data[i].images[0]%>"alt="img" id="imgs"></td>
        <td><%=data[i].brand_name%></td>
         <td><%=data[i].product_name%></td>
         <td><%=data[i].barcode%></td>
         <td><%=data[i].weight%> kg</td>
         <td><%=data[i].description%></td>
        <td>
            
            <% if(data[i].act_status==1) { %>
                <a href="/public/update_master_product_status/<%= data[i]._id%>"  class="btn btn-warning">Deactive</a>
                <% } else { %>
                   <a href="/public/update_master_product_status/<%= data[i]._id%>" class="btn btn-success">Active</a>
                   
                    <% } %>
                </td>
                  
       
<td><a href="/public/update_master_product/<%= data[i]._id%>" class="btn btn-primary"><i class="fa fa-edit"></i></a></td>

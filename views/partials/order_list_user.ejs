    <section class="content dashboard">
        <div class="main-graph">
            <div class="row clearfix">
                <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                    <div class="graph-area">
                        <div id="line_chart"></div>
                    </div> 
                </div> 
            </div>
        </div>
        <!-- #END# Main Graph-->
        <div class="page-heading">
            <h1>Order List</h1>
            <ol class="breadcrumb">
                <li><a href="/public/index">Home</a></li>
                <li class="active">Order List</li>
            </ol>
        </div>

        <div class="panel panel-default">
            
            <div class="panel-heading"></div>
           
                <div class="panel-body">
                    <div class="table-responsive">
                        <table id="myTable" class="table js-exportable table-bordered">
                            
                        <thead>
                            <tr> 
                                <th>S.No</th>
                                <th>User Image</th>
                                <th>User Name</th>
                                <th>Mobile No</th>
                                <th>Address</th>
                                <th>Shop Name</th>
                                <th>No of Product</th>    
                                <th>Total Ammount</th>
                                <th>Order Date</th>
                                <th>Order Status</th>
                                <!--<th>Action</th>
                                    <th>Delete</th>-->
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i < data.length; i++) { %>
                            <tr>
                                <td><%= i+1%></td>
                                <td><img src="/uploads/<%=data[i].userId.image%>"alt="img" id="imgs"></td>
                                <td><%=data[i].userId.user_name%></td>
                                <td><%=data[i].userId.phone%></td>
                                <td><%=data[i].addressId.address%></td>
                                <td><%=data[i].shopId.shop_name%></td>
                                <td><%= data[i].cartId ? data[i].cartId.products.length : '0' %></td>
                                <td><%=data[i].grand_total%></td>
                                <td><%=data[i].order_date%></td>
                                <td><%=data[i].order_status%></td>
                                <!--<td><a href="/public/edit_order/<%=data[i]._id%>" class="btn btn-primary">Edit</a></td>
                                <td><a href="/public/delete_order/<%=data[i]._id%>" class="btn btn-danger">Delete</a></td>-->
                            </tr>
                            <% } %>
                        </tbody> 
                            
                        </table>
                </div>
            </div>
        </div>
    <!-- #END# Task data -->
    </div>
</section>
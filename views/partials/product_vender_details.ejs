    <section class="content dashboard">
        <div class="main-graph">
            <div class="row clearfix">
                <div class="col-xs-12 col-sm-6 col-md-8 col-lg-10 padding-0 overflow-hidden">
                    <div class="graph-area">
                        <div id="line_chart"></div>
                    </div>
                </div>
    
            </div>
        </div>
        <!-- #END# Main Graph-->
        <div class="page-heading">
            <h1>Product List ( <%=list.shop_name%> )</h1>
            <ol class="breadcrumb">
                <li><a href="/public/index">Home</a></li>
                <li class="active">Product</li>
            </ol>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <button class="btn btn-info add-new"><i class="fa fa-plus"></i> <a href="/public/create_master_product">Add
                        New</a></button>
                <button class="btn btn-info add-new"><i class="fa fa-plus"></i> <a href="/public/csvfile_vender">CSV file
                        Upload</a></button>
            </div>
    
    
    
            <div class="panel-body"> 
                <ul class="nav nav-tabs" role="tablist">
                     <li>
                            <a href="/public/shop_vender_menu/<%=list._id%>" role="tab" >Shop Details</a></li>
                            <li><a href="/public/push_notification_service/<%=list._id%>" role="tab" >Push notification</a></li>
                            <li><a href="/public/delivery_boy_vender_list/<%=list._id%>" role="tab" >Delivery boy</a></li>
                            <li><a href="/public/user_qr_details/<%=list._id%>" role="tab" >Users</a></li>
                            <li class="active"><a href="/public/product_vender_list/<%=list._id%>"" role="tab" >Products</a></li>
                            <li><a href="/public/shop_vender_banner_list/<%=list._id%>" role="tab" >Banner</a></li>
                            <li><a href="/public/report_vender_details/<%=list._id%>" role="tab" >Report</a></li>
                           
                            <li><a href="/public/order_vender_details/<%=list._id%>" role="tab" >Order</a></li>
                            <li><a href="/public/vender_setting_details/<%=list._id%>" role="tab" >Setting</a></li>
                </ul>
                <br>
                <div class="table-responsive">
                    <table id="myTable" class="table js-exportable table-bordered">
    
                        <thead>
                            <tr>
                                <th>S.No</th>
                                <th>Image</th>
                                <th>ProductId</th>
                                <th>Product name</th>
                                <th>Category</th>
                                <th>SubCategory</th>
                                <th>Barcode</th>
                                <th>Weight</th>
                                <th>MRP</th>
                                <th>Price</th>
                                <th>discount</th>
                                <th>Gst</th>
                                <th>Description</th>
                                <th>act_status</th>
                                <th>like_status</th>
                                <th>stock_status</th>
                                <th>Status</th>
                                <th>Update</th>
                                <!-- <th>Delete</th> -->
                            </tr>
                        </thead>
                        <tbody>
                            <% for(var i=0; i < data.length; i++) { %>
    
                                <tr>
                                    <td>
                                        <%= i+1%>
                                    </td>
                                    
                                    <td><img src="/uploads/<%=data[i].images[0]%>" alt="img" id="imgs"></td>
                                    <td>
                                        <%=data[i]._id%>
                                    </td>
                                    <td>
                                        <%=data[i].products_name%>
                                    </td>
                                    <td>
                                        <%=data[i].category%>
                                    </td>
                                    <td>
                                        <%=data[i].subcategory%>
                                    </td>
                                    <td>
                                        <%=data[i].barcodes%>
                                    </td>
                                    <td>
                                        <%=data[i].variants%> kg
                                    </td>
                                    <td>
                                        <%=data[i].mrp_price%>
                                    </td>
                                    <td>
                                        <%=data[i].sale_price%>
                                    </td>
                                    <td>
                                        <%=data[i].gst%>
                                    </td>
                                    <td>
                                        <%=data[i].discount%>
                                    </td>
                                    <td>
                                        <%=data[i].description%>
                                    </td>
                                    <td>
                                        <%=data[i].act_status%>
                                    </td>
                                    <td>
                                        <%=data[i].like_status%>
                                    </td>
                                     <td>
                                        <%=data[i].stock_status%>
                                    </td>
                                    <td> 
     
                                        <% if(data[i].act_status==1) { %>
                                            <a href="/public/update_vender_product_status/<%=data[i]._id%>"
                                                class="btn btn-warning">Deactive</a>
                                            <% } else { %>
                                                <a href="/public/update_vender_product_status/<%=data[i]._id%>"
                                                    class="btn btn-success">Active</a>
    
                                                <% } %>
                                    </td>
    
    
                                    <td><a href="/public/edit_vender_product/<%=data[i]._id%>" class="btn btn-primary"><i
                                                class="fa fa-edit"></i></a></td>
                                    <!-- <td><a href ="/public/delete_product/<%= data[i].product_id%>"class="btn btn-circle  btn-sm  btn-danger"><i class="fa fa-trash"></i></a></td> -->
    
    
                                </tr>
                                <% } %>
                        </tbody>
    
                    </table>
                </div>
            </div>
        </div>
        <!-- #END# Task data -->
        </div>
    </section>